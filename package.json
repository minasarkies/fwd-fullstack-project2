{
  "name": "storefront-backend-project",
  "version": "1.0.0",
  "description": "Full Stack JavaScript Developer Nanodegree - Project 2",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "db-migrate db:create storefront_test && db-migrate --env test up && ENV=test jasmine-ts && db-migrate --env test down && db-migrate db:drop storefront_test",
    "dev": "db-migrate up && jasmine-ts && db-migrate down",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc '**/**/*.js' --write",
    "build": "npx tsc",
    "startmon": "nodemon dist/server.js",
    "lint": "eslint --fix --ignore-pattern 'node_modules/*' --ext **/**/**.ts ",
    "jasmine": "jasmine",
    "clean": "npm run prettier & npm run lint",
    "unittest": "npm run build && npm run jasmine"
  },
  "keywords": [],
  "author": "Mina S",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "generate-gitignore": "^0.2.6",
    "jasmine": "^3.10.0",
    "jasmine-es6": "^0.4.3",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "supertest": "^6.1.6",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pg": "^8.6.1",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-node": "^10.8.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.9.3"
  },
  "type": "commonjs"
}
